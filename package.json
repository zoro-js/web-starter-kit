{
  "name": "zoro-kit",
  "version": "0.0.31",
  "description": "",
  "main": "src/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "env": "env",
    "clean": "run-p -lns clean:*",
    "clean:css": "rimraf dist/css",
    "clean:js": "rimraf dist/js",
    "clean-dist": "rimraf dist",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint --color . ; exit 0",
    "prebuild:css": "npm run --silent clean:css",
    "prebuild:js": "npm run --silent clean:js",
    "build": "run-p -lns build:js build:css",
    "build:favicon": "node build/favicon.js",
    "build:css": "postcss --config build/postcss.js",
    "build:js": "webpack --config build/webpack.config.app.js",
    "build:js:stats": "npm run build:js -- --profile --json > dist/webpack-stats.json",
    "watch": "run-p watch:*",
    "watch:css": "npm run build:css --silent -- --watch",
    "watch:js": "npm run build:js --silent -- --watch",
    "watch-:lint": "watch 'npm run lint' -du --interval=0.25 --ignoreDirectoryPattern='/third|submodule|dist|node_modules/i' --filter=build/watchFilter.eslint.js",
    "dev": "npm run watch",
    "preprod": "npm run clean-dist",
    "prod": "run-p prod:*",
    "prod:js": "cross-env NODE_ENV=production npm run build:js",
    "prod:css": "cross-env NODE_ENV=production npm run build:css",
    "server": "puer & npm run dev",
    "archive": "git archive --format zip -o dist/regular-strap.zip master src dist",
    "prepub": "npm run dist",
    "foo": "NODE_ENV=production node build/foo.js"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "autoprefixer": "^6.4.0",
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-es3-member-expression-literals": "^6.8.0",
    "babel-plugin-transform-es3-property-literals": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-3": "^6.11.0",
    "babel-runtime": "^6.11.6",
    "cross-env": "^2.0.0",
    "css-loader": "^0.24.0",
    "cssnano": "^3.7.4",
    "eslint-config-zorojs": "*",
    "eslint-loader": "^1.5.0",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.22.0",
    "json-loader": "^0.5.4",
    "npm-run-all": "^3.0.0",
    "postcss": "^5.1.2",
    "postcss-calc": "^5.3.1",
    "postcss-loader": "^0.11.0",
    "postcss-sprites": "^4.0.0",
    "precss": "^1.4.0",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "watch": "^0.19.2",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.15.0",
    "yaml-loader": "^0.4.0",
    "zoro-globby": "*",
    "zoro-postcss-cli": "*"
  }
}
