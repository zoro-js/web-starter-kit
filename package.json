{
  "name": "zoro-kit",
  "version": "0.0.48",
  "description": "",
  "main": "src/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "env": "env",
    "clean": "run-p -lns clean:*",
    "clean:css": "rimraf dist/css",
    "clean:js": "rimraf dist/js",
    "clean:log": "rimraf logs/*.log",
    "clean-dist": "rimraf dist",
    "lint": "eslint --color . ; exit 0",
    "build": "run-p -lns build:js build:css",
    "build:favicon": "node build/favicon.js",
    "build:css": "postcss --config postcss.js",
    "build:js": "webpack",
    "build:js:stats": "webpack --profile --json > dist/webpack-stats.json",
    "watch": "run-p watch:*",
    "watch:css": "npm run build:css --silent -- --watch",
    "watch:js": "npm run build:js --silent -- --watch",
    "-watch:lint": "watch 'npm run lint' -du --interval=0.25 --ignoreDirectoryPattern='/third|submodule|dist|node_modules/i' --filter=build/watchFilter.eslint.js",
    "dev": "npm run watch",
    "dev:nosm": "cross-env NO_SOURCE_MAP=true npm run dev",
    "preprod": "npm run clean",
    "prod": "run-p prod:*",
    "prod:js": "cross-env NODE_ENV=production npm run build:js",
    "prod:css": "cross-env NODE_ENV=production npm run build:css",
    "server": "puer & npm run dev",
    "koa": "node server/require.js",
    "koa:debug": "node debug server/require.js",
    "pm2": "pm2 start bin/pm2.yaml --only node-wiki-dev",
    "pm2:stop": "pm2 stop bin/pm2.yaml --only node-wiki-dev",
    "pm2:restart": "pm2 restart bin/pm2.yaml --only node-wiki-dev",
    "pm2:delete": "pm2 delete bin/pm2.yaml --only node-wiki-dev",
    "prepm2:test": "npm run prod",
    "pm2:test": "pm2 start bin/pm2.yaml --only node-wiki-test",
    "pm2:test:stop": "pm2 stop bin/pm2.yaml --only node-wiki-test",
    "prepm2:prod": "npm run prod",
    "pm2:prod": "pm2 start bin/pm2.yaml --only node-wiki",
    "pm2:prod:stop": "pm2 stop bin/pm2.yaml --only node-wiki",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "run-p -lns koa dev"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "autoprefixer": "^6.4.1",
    "axios": "^0.14.0",
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-es3-member-expression-literals": "^6.8.0",
    "babel-plugin-transform-es3-property-literals": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-2": "^6.13.0",
    "babel-runtime": "^6.11.6",
    "cross-env": "^2.0.1",
    "css-loader": "^0.24.0",
    "cssnano": "^3.7.4",
    "eslint-config-zorojs": "*",
    "eslint-loader": "^1.5.0",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.22.0",
    "json-loader": "^0.5.4",
    "npm-run-all": "^3.1.0",
    "postcss": "^5.1.2",
    "postcss-calc": "^5.3.1",
    "postcss-cli": "^2.6.0",
    "postcss-import": "^8.1.2",
    "postcss-loader": "^0.11.1",
    "postcss-sprites": "^4.0.0",
    "precss": "^1.4.0",
    "raw-loader": "^0.5.1",
    "request-promise": "^4.1.1",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "watch": "^0.19.2",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.15.1",
    "yaml-loader": "^0.4.0",
    "zoro-globby": "*"
  }
}
